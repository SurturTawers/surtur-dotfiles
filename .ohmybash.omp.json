{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#89B4FA",
    "closer": "p:os",
    "lavender": "#B4BEFE",
    "os": "#ACB0BE",
    "pink": "#F5C2E7"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_user": true,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .UpstreamIcon }} {{ .User.Name }} {{ .HEAD }} {{ .Working }} ",
          "foreground": "#DBDBDB",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "aws",
          "style": "plain",
          "foreground": "#FF9900",
          "template": "AWS {{.Profile}}{{if .Region}}@{{.Region}}{{end}} "
        },
        {
          "type": "docker",
          "style": "plain",
          "foreground": "#328EEF",
          "template": " {{ .Context }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\ue5fe ",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "template": "{{ .Path }} ",
          "foreground": "#64A2FA",
          "type": "path",
          "style": "plain"
        },
        {
          "type": "angular",
          "style": "plain",
          "foreground": "#E90364",
          "template": " {{ .Full }} "
        },
        {
          "type": "php",
          "style": "plain",
          "foreground": "#8789de",
          "template": " {{ .Full }} "
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#6CA35E",
          "template": " {{ .Full }} "
        },
        {
          "type": "os",
          "style": "plain",
          "foreground": "#E75320",
          "template": "{{.Icon}}  "
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{.UserName}}@{{.HostName}} \uf105",
          "foreground": "p:closer",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
